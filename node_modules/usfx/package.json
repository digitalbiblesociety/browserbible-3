{
  "name": "usfx",
  "version": "0.1.3",
  "description": "Bible USFX parser",
  "main": "lib/bundle.js",
  "scripts": {
    "docs": "node_modules/.bin/jsdoc --readme ./README.md src/*.js && open out/index.html",
    "test": "node_modules/.bin/mocha --require babel-register test/*.js",
    "clean": "rm -rf lib && mkdir lib",
    "build-lib": "browserify main.js -t babelify --outfile lib/bundle.js",
    "build": "npm run clean && npm run build-lib"
  },
  "keywords": [
    "bible",
    "usfx",
    "osis",
    "xml",
    "scripture",
    "usfm"
  ],
  "author": "Kevin Turner",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "core-js": "^2.4.1",
    "mocha": "^3.0.2"
  },
  "dependencies": {
    "bible-passage-reference-parser": "^2.0.0",
    "clear": "0.0.1",
    "clui": "^0.3.1",
    "sax": "^1.2.1",
    "xmlstream2": "^1.1.1"
  }
}
